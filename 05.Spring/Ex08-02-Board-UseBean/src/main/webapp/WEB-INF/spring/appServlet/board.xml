<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
						http://www.springframework.org/schema/context 
						http://www.springframework.org/schema/context/spring-context-3.1.xsd">
						
	<bean id="deleteHandler" class="com.study.spring14.command.BDeleteCommand" />		
	<bean id="modifyHandler" class="com.study.spring14.command.BModifyCommand" />
	<bean id="replyHandler" class="com.study.spring14.command.BReplyCommand" />
	<bean id="replyViewHandler" class="com.study.spring14.command.BReplyViewCommand" />
	<bean id="writeHandler" class="com.study.spring14.command.BWriteCommand" />			


</beans>
